{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BasePortfolioApiUrl": "https://api.markhollingsworth.io",
  "BootstrapUrl": "https://getbootstrap.com/",
  "DotNetUrl": "https://dotnet.microsoft.com/en-us/download/dotnet/7.0/",
  "CSharpUrl": "https://learn.microsoft.com/en-us/dotnet/csharp/",
  "BlazorUrl": "https://dotnet.microsoft.com/en-us/apps/aspnet/web-apps/blazor",
  "JavascriptUrl": "https://developer.mozilla.org/en-US/docs/Web/javascript",
  "AzureUrl": "https://azure.microsoft.com/en-us/",
  "GithubUrl": "https://github.com/markwhollingsworth/Portfolio/",
  "ResumeUrl": "https://oldazroads.blob.core.windows.net/portfolio/docs/Mark%20Warren%20Hollingsworth.docx",

  "SourceFolder": "Styles",
  "TargetFolder": "wwwroot/css",
  "Arguments": "--style=compressed",
  "GenerateScopedCss": true,
  "ScopedCssFolders": [ "Views", "Pages", "Shared", "Components" ],
  "IncludePaths": [],

  // You can override specific options based on the build configuration
  "Configurations": {
    "Debug": { // These options apply only to Debug builds
      "Arguments": "--style=expanded"
    }
  }


}
